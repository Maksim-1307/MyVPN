{
    "server": "0.0.0.0",
    "server_port": 443,
    "password": "${SHADOWSOCKS_PASSWORD}",
    "method": "aes-256-gcm",
    "plugin": "v2ray-plugin",
    "plugin_opts": "server;tls;host=${DOMAIN_NAME};cert=/etc/letsencrypt/live/${DOMAIN_NAME}/fullchain.pem;key=/etc/letsencrypt/live/${DOMAIN_NAME}/privkey.pem"
}